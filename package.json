{
  "name": "fplfocalwebsite",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:start": "next dev",
    "dev": "concurrently \"next dev\" \"npm run db:connect:dev\"",
    "dev2": "concurrently \"next dev\" \"npm run db:connect:master\"",
    "build": "prisma generate && next build && pscale connect fplwebsitedatabase master",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "db:connect:dev": "pscale connect fplwebsitedatabase dev --port 3309",
    "db:connect:master": "pscale connect fplwebsitedatabase master --port 3309",
    "db:push": "npx prisma db push"
  },
  "dependencies": {
    "@fortawesome/free-brands-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@prisma/client": "^4.14.1",
    "@types/node": "18.15.11",
    "@types/react": "18.0.33",
    "@types/react-dom": "18.0.11",
    "bootstrap": "^5.2.3",
    "eslint": "8.37.0",
    "eslint-config-next": "^13.4.0",
    "next": "^13.4.0",
    "prisma": "^4.14.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "concurrently": "^8.0.1"
  }
}
